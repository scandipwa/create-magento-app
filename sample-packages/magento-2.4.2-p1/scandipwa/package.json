{
    "name": "scandipwa",
    "version": "0.0.1",
    "license": "OSL-3.0",
    "dependencies": {
        "@scandipwa/scandipwa": "4.4.0",
        "@scandipwa/scandipwa-scripts": "2.4.9",
        "@scandipwa/eslint-config": "0.1.25",
        "@tilework/mosaic": "0.1.4"
    },
    "scandipwa": {
        "type": "theme",
        "locales": {
            "en_US": true
        },
        "parentTheme": "@scandipwa/scandipwa",
